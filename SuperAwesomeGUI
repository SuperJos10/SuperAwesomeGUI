-- Function to stop script execution
local function killScriptFunction()
    -- Stop any ongoing processes, e.g., sound and skybox changes
    for _, v in ipairs(game.Workspace:GetChildren()) do
        if v:IsA(CSQzKVQTZMC({8715,11655,12285,11550,10500})) then
            v:Stop()
            v:Destroy()
        elseif v:IsA(CSQzKVQTZMC({8715,11235,12705})) then
            v:Destroy()
        end
    end
    -- Additional cleanup as needed
    
    print(CSQzKVQTZMC({8715,10395,11970,11025,11760,12180,3360,12180,10605,11970,11445,11025,11550,10185,12180,10605,10500,4830}))
end

-- Connect button click to killScriptFunction
killScriptButton.MouseButton1Click:Connect(killScriptFunction)
        
