-- Rainbow strobe effect function for frame background color
local rainbowColors = {
    Color3.fromRGB(255, 0, 0),
    Color3.fromRGB(255, 127, 0),
    Color3.fromRGB(255, 255, 0),
    Color3.fromRGB(0, 255, 0),
    Color3.fromRGB(0, 0, 255),
    Color3.fromRGB(75, 0, 130),
    Color3.fromRGB(148, 0, 211)
}

local rainbowInterval = 0.1  -- Time interval between color changes in seconds

local function rainbowStrobe()
    while true do
        for _, color in ipairs(rainbowColors) do
            frame.BackgroundColor3 = color
            wait(rainbowInterval)
        end
    end
end

-- Start rainbow strobe effect in a coroutine
coroutine.wrap(rainbowStrobe)()

